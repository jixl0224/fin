#Zookeeper=========================================================
snsoft.zk=true
snsoft.zk.connectString=127.0.0.1:2181
#snsoft.zk.connectString=127.0.0.1:2181,127.0.0.1:2182,127.0.0.1:2183
snsoft.zk.sessionTimeout=10000
#snsoft.zk.watcherClass=
dubbo.registryAddress=127.0.0.1:2181


#Redis=========================================================
#非集群
snsoft.redis=true
snsoft.redis.host=127.0.0.1
snsoft.redis.port=6379
#集群
#snsoft.redis.cluster.nodes=127.0.0.1:7000,127.0.0.1:7001,127.0.0.1:7002,127.0.0.1:7003,127.0.0.1:7004,127.0.0.1:7005

#Redis-分码服务===================================================
#snsoft.mc.redis=true
snsoft.mc.redis.host=127.0.0.1
snsoft.mc.redis.port=6379


#MongoTemplate=================================================
#系统默认
#1、日志存储及查询使用
snsoft.mongo=true
snsoft.mongo.uri=mongodb://127.0.0.1:27017/SW

#禁用Redis缓存通知
#SQL.DisableRedisSqlUpdateNotifier=true

#Auto
#登录回话时长，默认30分钟；单位秒
Login.Session.Interval=28800

#配置版本(默认值1.0.0)
cfg.ver=1.0.0

#配置系统的主属性文件
#优先级先后顺序
cfg.priority=file
#本地配置文件基目录
cfg.file.root=${verbose_path};/snsoft90/snsoft_adk
